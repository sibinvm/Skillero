                <label for="terms" class="form-check-label">
                  I agree to <a href="#" id="terms-link"> terms and conditions</a><span style="color: red;"> *</span>
                </label>
              </div>
              
              
              <button id="login-form-button" type="button" class="form-control btn btn-primary">SIGN UP</button>
            </div>
          </form>
        </div>
        <div class="modal-footer">
          <span>Already have an Account?</span><a href="#" id="login-signup" data-toggle="modal" data-target="#login-form" data-dismiss="modal">Log In</a>
        </div>
