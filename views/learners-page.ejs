  <script>
    $(".card-body").on('click', function(){
      value(this);
    });
    function value(elem){
      var namee = elem.innerText;
      
      localStorage.setItem("rupees", namee);
      
    }

    $(".card").on('click', function(){
      setImage(this);
    });
    function setImage(elem){
      var image = elem.children[0].getAttribute("src");
      localStorage.setItem("img", image);
    }
    
  </script>







